package oPPsConCept;


class Student {
	
	String name;
	int age;
	
	public void stdinfo() {
		
		System.out.println(this.name);
	}
}

public class ConstructorsC {

	public static void main(String[] args) {
		
		Student s1= new Student();
		
		s1.age = 26;
		s1.name = "aman";
		s1.stdinfo();
		

	}

}

package Test3;

public class PrimeNumbercheck {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		

		System.out.println("Enter a numer");
		
		int var1=13;
		
		for(int index=2;index>var1;index++) {
		
		int res1=var1%index;
		
		if(res1 == 0) {
			System.out.println("This is prime number" +res1);

		
		}else {
			System.out.println("This is even number" +res1);
		}
		}	

	}

}	

